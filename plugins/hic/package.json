{
  "name": "@jbrowse/plugin-hic",
  "version": "4.0.3",
  "description": "JBrowse 2 hic adapters, tracks, etc.",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "plugins/hic"
  },
  "author": "JBrowse Team",
  "main": "src/index.ts",
  "files": [
    "esm"
  ],
  "scripts": {
    "build": "yarn build:esm",
    "test": "cd ../..; jest --passWithNoTests plugins/hic",
    "prepublishOnly": "yarn test",
    "prepack": "yarn build",
    "prebuild": "yarn clean",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "clean": "rimraf esm *.tsbuildinfo"
  },
  "dependencies": {
    "@jbrowse/core": "^4.0.3",
    "@jbrowse/mobx-state-tree": "^5.5.0",
    "generic-filehandle2": "^2.0.18",
    "@jbrowse/plugin-linear-genome-view": "^4.0.3",
    "@mui/x-charts-vendor": "^8.23.0",
    "hic-straw": "^2.1.4",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1",
    "rxjs": "^7.8.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "main": "esm/index.js",
    "types": "esm/index.d.ts",
    "exports": {
      ".": {
        "types": "./esm/index.d.ts",
        "import": "./esm/index.js"
      }
    }
  },
  "sideEffects": false
}
