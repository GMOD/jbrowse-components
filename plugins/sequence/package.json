{
  "name": "@jbrowse/plugin-sequence",
  "version": "1.7.8",
  "description": "JBrowse 2 sequence adapters, tracks, etc.",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "plugins/sequence"
  },
  "author": "JBrowse Team",
  "distMain": "dist/index.js",
  "srcMain": "src/index.ts",
  "main": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "babel src --root-mode upward --out-dir dist --extensions .ts,.js,.tsx,.jsx",
    "test": "cd ../..; jest plugins/sequence",
    "prepublishOnly": "yarn test",
    "prepack": "yarn build; yarn useDist",
    "postpack": "yarn useSrc",
    "useDist": "node ../../scripts/useDist.js",
    "useSrc": "node ../../scripts/useSrc.js",
    "postbuild": "tsc --build tsconfig.build.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@gmod/indexedfasta": "^2.0.2",
    "@gmod/twobit": "^1.1.12",
    "abortable-promise-cache": "^1.5.0",
    "generic-filehandle": "^2.2.2"
  },
  "peerDependencies": {
    "@jbrowse/core": "^1.0.0",
    "@jbrowse/plugin-linear-genome-view": "^1.0.0",
    "@jbrowse/plugin-wiggle": "^1.0.0",
    "@mui/material": "^5.0.0",
    "mobx-react": "^6.0.0",
    "mobx-state-tree": "3.14.1",
    "prop-types": "^15.0.0",
    "react": ">=16.8.0",
    "rxjs": "^6.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
