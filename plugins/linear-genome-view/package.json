{
  "name": "@jbrowse/plugin-linear-genome-view",
  "version": "4.0.3",
  "description": "JBrowse 2 linear genome view",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "plugins/linear-genome-view"
  },
  "author": "JBrowse Team",
  "main": "src/index.ts",
  "files": [
    "esm"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn build:esm",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "clean": "rimraf esm *.tsbuildinfo",
    "test": "cd ../..; jest --passWithNoTests plugins/linear-genome-view",
    "prepublishOnly": "yarn test",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@jbrowse/core": "^4.0.3",
    "@jbrowse/mobx-state-tree": "^5.5.0",
    "@jbrowse/product-core": "^4.0.3",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@types/file-saver-es": "^2.0.3",
    "copy-to-clipboard": "^3.3.3",
    "file-saver-es": "^2.0.5",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "main": "esm/index.js",
    "types": "esm/index.d.ts",
    "exports": {
      ".": {
        "types": "./esm/index.d.ts",
        "import": "./esm/index.js"
      },
      "./BaseLinearDisplay/types": {
        "types": "./esm/BaseLinearDisplay/types.d.ts",
        "import": "./esm/BaseLinearDisplay/types.js"
      },
      "./BaseLinearDisplay/models/serverSideRenderedBlock": {
        "types": "./esm/BaseLinearDisplay/models/serverSideRenderedBlock.d.ts",
        "import": "./esm/BaseLinearDisplay/models/serverSideRenderedBlock.js"
      },
      "./BaseLinearDisplay/components/util": {
        "types": "./esm/BaseLinearDisplay/components/util.d.ts",
        "import": "./esm/BaseLinearDisplay/components/util.js"
      },
      "./LinearGenomeView/types": {
        "types": "./esm/LinearGenomeView/types.d.ts",
        "import": "./esm/LinearGenomeView/types.js"
      }
    }
  },
  "sideEffects": false,
  "types": "esm/index.d.ts",
  "exports": {
    ".": "./src/index.ts",
    "./BaseLinearDisplay/types": "./src/BaseLinearDisplay/types.ts",
    "./BaseLinearDisplay/models/serverSideRenderedBlock": "./src/BaseLinearDisplay/models/serverSideRenderedBlock.ts",
    "./BaseLinearDisplay/components/util": "./src/BaseLinearDisplay/components/util.ts",
    "./LinearGenomeView/types": "./src/LinearGenomeView/types.ts"
  }
}
