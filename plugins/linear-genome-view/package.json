    "react-error-boundary": "^4.0.3"
  },
  "peerDependencies": {
    "@jbrowse/core": "^2.0.0",
    "@mui/material": "^5.0.0",
    "mobx": "^6.0.0",
    "mobx-react": "^9.0.0",
    "mobx-state-tree": "^5.0.0",
    "react": ">=16.8.0",
