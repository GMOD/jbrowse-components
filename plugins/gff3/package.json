{
  "name": "@jbrowse/plugin-gff3",
  "version": "1.7.3",
  "description": "JBrowse 2 gff3.",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "plugins/gff3"
  },
  "author": "JBrowse Team",
  "distMain": "dist/index.js",
  "srcMain": "src/index.ts",
  "main": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "babel src --root-mode upward --out-dir dist --extensions .ts,.js,.tsx,.jsx",
    "postbuild": "tsc --build tsconfig.build.json",
    "test": "cd ../..; jest plugins/gff3 --passWithNoTests",
    "prepublishOnly": "yarn test",
    "prepack": "yarn build; yarn useDist",
    "postpack": "yarn useSrc",
    "useDist": "node ../../scripts/useDist.js",
    "useSrc": "node ../../scripts/useSrc.js"
  },
  "dependencies": {
    "@flatten-js/interval-tree": "^1.0.15",
    "@gmod/bgzf-filehandle": "^1.4.3",
    "@gmod/gff": "^1.2.0",
    "@gmod/tabix": "^1.5.2"
  },
  "peerDependencies": {
    "@jbrowse/core": "^1.0.0",
    "@jbrowse/plugin-linear-genome-view": "^1.0.0",
    "@material-ui/core": "^4.12.2",
    "mobx-react": "^6.0.0",
    "mobx-state-tree": "3.14.1",
    "prop-types": "^15.0.0",
    "react": ">=16.8.0",
    "rxjs": "^6.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
