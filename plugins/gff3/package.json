{
  "name": "@jbrowse/plugin-gff3",
  "version": "3.7.0",
  "description": "JBrowse 2 gff3.",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "plugins/gff3"
  },
  "author": "JBrowse Team",
  "distMain": "dist/index.js",
  "srcMain": "src/index.ts",
  "main": "src/index.ts",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "npm-run-all build:*",
    "test": "cd ../..; jest --passWithNoTests plugins/gff3 --passWithNoTests",
    "prepublishOnly": "yarn test",
    "prepack": "yarn build && yarn useDist",
    "postpack": "yarn useSrc",
    "useDist": "node ../../scripts/useDist.js",
    "useSrc": "node ../../scripts/useSrc.js",
    "prebuild": "npm run clean",
    "build:esm": "tsc tsconfig.build.esm.json",
    "build:commonjs": "tsc tsconfig.build.commonjs.json",
    "clean": "rimraf dist esm *.tsbuildinfo"
  },
  "dependencies": {
    "@flatten-js/interval-tree": "^2.0.0",
    "@gmod/bgzf-filehandle": "^4.0.0",
    "@gmod/tabix": "^3.0.1",
    "@jbrowse/core": "^3.7.0",
    "@jbrowse/plugin-linear-genome-view": "^3.7.0",
    "@mui/material": "^7.0.0",
    "gff-nostream": "^1.3.3",
    "mobx": "^6.0.0",
    "mobx-state-tree": "^5.0.0",
    "rxjs": "^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "distModule": "esm/index.js",
  "srcModule": "src/index.ts",
  "module": ""
}
