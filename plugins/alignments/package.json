    "@jbrowse/core": "workspace:^",
    "@jbrowse/mobx-state-tree": "^5.5.0",
    "@jbrowse/plugin-linear-genome-view": "workspace:^",
    "@jbrowse/plugin-wiggle": "workspace:^",
    "@jbrowse/sv-core": "workspace:^",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/x-charts-vendor": "^8.23.0",
    "canvas2svg": "^1.0.16",
    "copy-to-clipboard": "^3.3.3",
    "fast-deep-equal": "^3.1.3",
    "generic-filehandle2": "^2.0.18",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1",
    "rxjs": "^7.8.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "main": "esm/index.js",
    "types": "esm/index.d.ts",
    "exports": {
      ".": {
