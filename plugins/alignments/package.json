{
  "name": "@jbrowse/plugin-alignments",
  "version": "3.7.0",
  "description": "JBrowse 2 alignments adapters, tracks, etc.",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "plugins/alignments"
  },
  "author": "JBrowse Team",
  "distMain": "dist/index.js",
  "srcMain": "src/index.ts",
  "main": "src/index.ts",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build": "npm-run-all build:*",
    "test": "cd ../..; jest --passWithNoTests plugins/alignments",
    "prepublishOnly": "pnpm test",
    "prepack": "pnpm build && pnpm useDist",
    "postpack": "pnpm useSrc",
    "useDist": "node ../../scripts/useDist.js",
    "useSrc": "node ../../scripts/useSrc.js",
    "prebuild": "pnpm clean",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:commonjs": "tsc -p tsconfig.build.commonjs.json",
    "clean": "rimraf dist esm *.tsbuildinfo"
  },
  "dependencies": {
    "@gmod/bam": "^7.1.15",
    "@gmod/cram": "^7.0.3",
    "@jbrowse/core": "workspace:^",
    "@jbrowse/mobx-state-tree": "^5.4.7",
    "@jbrowse/plugin-linear-genome-view": "workspace:^",
    "@jbrowse/plugin-wiggle": "workspace:^",
    "@jbrowse/sv-core": "workspace:^",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "canvas2svg": "^1.0.16",
    "copy-to-clipboard": "^3.3.1",
    "fast-deep-equal": "^3.1.3",
    "generic-filehandle2": "^2.0.17",
    "mobx": "^6.0.0",
    "mobx-react": "^9.0.0",
    "rxjs": "^7.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "distModule": "esm/index.js",
  "srcModule": "src/index.ts",
  "module": ""
}
