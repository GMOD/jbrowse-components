{
  "name": "@jbrowse/plugin-legacy-jbrowse",
  "version": "4.0.3",
  "description": "JBrowse 2 plugin for connecting to and reading JBrowse 1 data",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "plugins/legacy-jbrowse"
  },
  "author": "JBrowse Team",
  "main": "src/index.ts",
  "files": [
    "esm"
  ],
  "scripts": {
    "build": "yarn build:esm",
    "test": "cd ../..; jest --passWithNoTests plugins/legacy-jbrowse",
    "prepublishOnly": "yarn test",
    "prepack": "yarn build",
    "prebuild": "yarn clean",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "clean": "rimraf esm *.tsbuildinfo"
  },
  "dependencies": {
    "@gmod/nclist": "^3.0.4",
    "@jbrowse/core": "^4.0.3",
    "@jbrowse/mobx-state-tree": "^5.5.0",
    "generic-filehandle2": "^2.0.18",
    "mobx": "^6.15.0",
    "rxjs": "^7.8.2",
    "set-value": "^4.1.0"
  },
  "publishConfig": {
    "access": "public",
    "main": "esm/index.js",
    "types": "esm/index.d.ts",
    "exports": {
      ".": {
        "types": "./esm/index.d.ts",
        "import": "./esm/index.js"
      }
    }
  },
  "sideEffects": false,
  "types": "esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js"
    }
  }
}
