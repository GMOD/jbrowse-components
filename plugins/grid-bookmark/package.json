    "directory": "plugins/grid-bookmark"
  },
  "author": "JBrowse Team",
  "distMain": "dist/index.js",
  "srcMain": "src/index.ts",
  "main": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm-run-all build:*",
