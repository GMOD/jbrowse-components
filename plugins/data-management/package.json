{
  "name": "@jbrowse/plugin-data-management",
  "version": "1.7.7",
  "description": "JBrowse 2 linear genome view",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "plugins/data-management"
  },
  "author": "JBrowse Team",
  "distMain": "dist/index.js",
  "srcMain": "src/index.ts",
  "main": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "babel src --root-mode upward --out-dir dist --extensions .ts,.js,.tsx,.jsx",
    "test": "cd ../..; jest plugins/data-management",
    "prepublishOnly": "yarn test",
    "prepack": "yarn build; yarn useDist",
    "postpack": "yarn useSrc",
    "useDist": "node ../../scripts/useDist.js",
    "useSrc": "node ../../scripts/useSrc.js",
    "postbuild": "tsc --build tsconfig.build.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@gmod/ucsc-hub": "^0.1.3",
    "@mui/icons-material": "^5.0.1",
    "clsx": "^1.1.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-vtree": "^3.0.0-beta.1",
    "react-window": "^1.8.6"
  },
  "peerDependencies": {
    "@jbrowse/core": "^1.0.0",
    "@jbrowse/plugin-config": "^1.0.0",
    "@mui/material": "^5.0.0",
    "@mui/lab": "^5.0.0-alpha.45",
    "mobx-react": "^6.0.0",
    "mobx-state-tree": "3.14.1",
    "prop-types": "^15.0.0",
    "react": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
