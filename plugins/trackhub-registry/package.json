    "useDist": "node ../../scripts/useDist.js",
    "useSrc": "node ../../scripts/useSrc.js",
    "postbuild": "tsc --build tsconfig.build.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@gmod/ucsc-hub": "^0.1.3",
    "@mui/icons-material": "^5.0.1"
  },
  "peerDependencies": {
    "@jbrowse/core": "^1.0.0",
    "@mui/material": "^5.0.0",
    "dompurify": "^2.0.11",
    "mobx-react": "^7.0.0",
    "mobx-state-tree": "^5.0.0",
    "prop-types": "^15.0.0",
    "react": ">=16.8.0",
    "mobx": "^6.0.0"
  },
  "private": true
}
