{
  "name": "@jbrowse/product-core",
  "version": "3.7.0",
  "sideEffects": false,
  "description": "JBrowse 2 code shared between products but not used by plugins",
  "keywords": [
    "jbrowse",
    "jbrowse2",
    "bionode",
    "biojs",
    "genomics"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "packages/product-core"
  },
  "author": "JBrowse Team",
  "distMain": "dist/index.js",
  "distModule": "esm/index.js",
  "srcMain": "src/index.ts",
  "srcModule": "src/index.ts",
  "main": "src/index.ts",
  "module": "",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:commonjs": "tsc -p tsconfig.build.commonjs.json",
    "build": "yarn build:esm && yarn build:commonjs",
    "test": "cd ../..; jest --passWithNoTests packages/product-core",
    "clean": "rimraf dist esm *.tsbuildinfo",
    "prebuild": "yarn clean",
    "prepack": "yarn build && yarn useDist",
    "postpack": "yarn useSrc",
    "useDist": "node ../../scripts/useDist.js",
    "useSrc": "node ../../scripts/useSrc.js"
  },
  "dependencies": {
    "@jbrowse/core": "^3.7.0",
    "@jbrowse/mobx-state-tree": "^5.4.7",
    "@mui/material": "^7.0.0",
    "copy-to-clipboard": "^3.3.1",
    "librpc-web-mod": "^2.1.0",
    "mobx": "^6.0.0",
    "mobx-react": "^9.0.0",
    "tss-react": "^4.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
