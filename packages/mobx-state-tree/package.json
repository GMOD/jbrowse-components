{
  "name": "@jbrowse/mobx-state-tree",
  "version": "5.6.0",
  "description": "Opinionated, transactional, MobX powered state container",
  "distMain": "dist/index.js",
  "distModule": "esm/index.mjs",
  "srcMain": "src/index.ts",
  "srcModule": "src/index.ts",
  "main": "src/index.ts",
  "module": "",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist lib",
    "prebuild": "yarn clean",
    "build:esm": "tsup  --format esm src/index.ts --dts -d esm",
    "build:es5": "tsup  src/index.ts --dts",
    "build": "yarn build:es5 && yarn build:esm",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/@jbrowse/@jbrowse/mobx-state-tree.git"
  },
  "author": "Michel Weststrate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/@jbrowse/@jbrowse/mobx-state-tree/issues"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "ts-essentials": "^10.0.4",
    "tsup": "^8.4.0"
  },
  "peerDependencies": {
    "mobx": "^6.3.0"
  }
}
