{
  "name": "@jbrowse/embedded-core",
  "version": "3.7.0",
  "description": "JBrowse 2 code shared between embedded products",
  "keywords": [
    "jbrowse",
    "jbrowse2",
    "bionode",
    "biojs",
    "genomics"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "packages/embedded-core"
  },
  "author": "JBrowse Team",
  "distMain": "dist/index.js",
  "distModule": "esm/index.js",
  "srcMain": "src/index.ts",
  "srcModule": "src/index.ts",
  "main": "src/index.ts",
  "module": "",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:commonjs": "tsc -p tsconfig.build.commonjs.json",
    "build": "pnpm build:esm && pnpm build:commonjs",
    "test": "cd ../..; jest --passWithNoTests packages/embedded-core",
    "clean": "rimraf dist esm *.tsbuildinfo",
    "prebuild": "pnpm clean",
    "prepack": "pnpm build && pnpm useDist",
    "postpack": "pnpm useSrc",
    "useDist": "node ../../scripts/useDist.js",
    "useSrc": "node ../../scripts/useSrc.js"
  },
  "dependencies": {
    "@jbrowse/core": "workspace:^",
    "@jbrowse/mobx-state-tree": "^5.5.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
