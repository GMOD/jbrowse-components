{
  "name": "@jbrowse/app-core",
  "version": "3.7.0",
  "description": "JBrowse 2 code shared between the 'full featured' apps e.g. jbrowse-web and jbrowse-desktop",
  "keywords": [
    "jbrowse",
    "jbrowse2",
    "bionode",
    "biojs",
    "genomics"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "packages/app-core"
  },
  "author": "JBrowse Team",
  "main": "src/index.ts",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:commonjs": "tsc -p tsconfig.build.commonjs.json",
    "build": "pnpm build:esm && pnpm build:commonjs",
    "test": "cd ../..; jest --passWithNoTests packages/app-core",
    "clean": "rimraf dist esm *.tsbuildinfo",
    "prebuild": "pnpm clean",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@jbrowse/core": "workspace:^",
    "@jbrowse/mobx-state-tree": "^5.5.0",
    "@jbrowse/product-core": "workspace:^",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "dockview-react": "^4.13.1",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./esm/index.js",
        "require": "./dist/index.js"
      }
    }
  },
  "sideEffects": false
}
