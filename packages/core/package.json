{
  "name": "@jbrowse/core",
  "version": "3.7.0",
  "description": "JBrowse 2 core libraries used by plugins",
  "keywords": [
    "jbrowse",
    "jbrowse2",
    "bionode",
    "biojs",
    "genomics"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "packages/core"
  },
  "author": "JBrowse Team",
  "main": "src/index.ts",
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "test": "cd ../..; jest --passWithNoTests packages/core",
    "storybook": "storybook dev --port 6006",
    "storybook:build": "storybook build",
    "coverage": "pnpm test --coverage",
    "clean": "rimraf dist esm *.tsbuildinfo",
    "prebuild": "pnpm clean",
    "prepack": "pnpm build",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:commonjs": "tsc -p tsconfig.build.commonjs.json",
    "build": "pnpm build:esm && pnpm build:commonjs"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/serialize": "^1.3.3",
    "@emotion/utils": "^1.4.2",
    "@floating-ui/react": "^0.27.16",
    "@gmod/abortable-promise-cache": "^3.0.4",
    "@gmod/bgzf-filehandle": "^6.0.9",
    "@gmod/http-range-fetcher": "^5.0.6",
    "@jbrowse/jexl": "^3.0.1",
    "@jbrowse/mobx-state-tree": "^5.5.0",
    "@jbrowse/quick-lru": "^7.3.5",
    "@leeoniya/ufuzzy": "^1.0.19",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/x-data-grid": "^8.23.0",
    "canvas-sequencer-ts": "^3.1.3",
    "canvas2svg": "^1.0.16",
    "colord": "^2.9.3",
    "copy-to-clipboard": "^3.3.3",
    "deepmerge": "^4.3.1",
    "detect-node": "^2.1.0",
    "dompurify": "^3.3.1",
    "escape-html": "^1.0.3",
    "fast-deep-equal": "^3.1.3",
    "file-saver-es": "^2.0.5",
    "generic-filehandle2": "^2.0.18",
    "librpc-web-mod": "^2.1.1",
    "load-script": "^2.0.0",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1",
    "react-draggable": "^4.5.0",
    "rxjs": "^7.8.2",
    "source-map-js": "^1.2.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./esm/index.js",
        "require": "./dist/index.js"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "import": "./esm/*.js",
        "require": "./dist/*.js"
      }
    }
  },
  "sideEffects": false
}
