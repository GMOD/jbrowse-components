{
  "name": "@jbrowse/core",
  "version": "1.7.11",
  "description": "JBrowse 2 core libraries used by plugins",
  "keywords": [
    "jbrowse",
    "jbrowse2",
    "bionode",
    "biojs",
    "genomics"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "packages/core"
  },
  "author": "JBrowse Team",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all build:*",
    "build:esm": "tsc --build tsconfig.build.esm.json",
    "build:es5": "tsc --build tsconfig.build.es5.json",
    "clean": "rimraf dist esm *.tsbuildinfo",
    "test": "cd ../..; jest packages/core",
    "coverage": "yarn test --coverage",
    "prepack": "yarn build",
    "postbuild": "tsc -b tsconfig.build.json"
  },
  "exports": {
    "./PluginManager": {
      "require": "./dist/PluginManager.js",
      "import": "./esm/PluginManager.js"
    },
    "./configuration": {
      "require": "./dist/configuration/index.js",
      "import": "./esm/configuration/index.js"
    },
    "./ui": {
      "require": "./dist/ui/index.js",
      "import": "./esm/ui/index.js"
    },
    "./util": {
      "requtilre": "./dist/util/index.js",
      "import": "./esm/util/index.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@mui/icons-material": "^5.0.1",
    "abortable-promise-cache": "^1.5.0",
    "canvas-sequencer": "^3.1.0",
    "canvas2svg": "^1.0.16",
    "clone": "^2.1.2",
    "clsx": "^1.0.4",
    "color": "^3.1.3",
    "copy-to-clipboard": "^3.3.1",
    "deepmerge": "^4.2.2",
    "detect-node": "^2.1.0",
    "dompurify": "^2.3.4",
    "escape-html": "^1.0.3",
    "fast-deep-equal": "^3.1.3",
    "generic-filehandle": "^2.2.2",
    "http-range-fetcher": "^1.4.0",
    "is-object": "^1.0.1",
    "jexl": "^2.3.0",
    "json-stable-stringify": "^1.0.1",
    "librpc-web-mod": "^1.1.5",
    "load-script2": "^2.0.5",
    "material-ui-popup-state": "^3.0.0",
    "rbush": "^3.0.1",
    "react-error-boundary": "^3.0.0",
    "react-intersection-observer": "^9.3.0",
    "react-use-measure": "^2.1.1",
    "shortid": "^2.2.13",
    "svg-path-generator": "^1.1.0"
  },
  "peerDependencies": {
    "@mui/material": "^5.0.0",
    "@mui/x-data-grid": "^5.0.0",
    "mobx": "^6.0.0",
    "mobx-react": "^7.0.0",
    "mobx-state-tree": "^5.0.0",
    "prop-types": "^15.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "rxjs": "^6.0.0",
    "tss-react": "^3.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
