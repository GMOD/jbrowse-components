    "lint": "eslint --ext .js,.ts,.jsx,.tsx src",
    "prepack": "yarn build",
    "start-oauth-server": "ts-node --project tsconfig.oauth.json src/tests/OAuthServer/app.ts",
    "start-basicauth-server": "ts-node --project tsconfig.oauth.json src/tests/HTTPBasicAuthServer/app.ts"
