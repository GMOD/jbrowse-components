{
  "name": "@jbrowse/cli",
  "version": "4.0.4",
  "type": "module",
  "description": "A command line tool for working with JBrowse 2",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "Apache-2.0",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "products/jbrowse-cli"
  },
  "author": "JBrowse Team",
  "main": "dist/index.js",
  "bin": {
    "jbrowse": "./dist/bin.js"
  },
  "files": [
    "/bin",
    "/bundle",
    "/dist"
  ],
  "engines": {
    "node": ">=18.3.0"
  },
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "tsc && webpack -c webpack.config.mjs",
    "clean": "rimraf bundle",
    "prepack": "pnpm clean && pnpm build && pnpm generate-readme",
    "generate-readme": "./generate_readme.sh > README.md"
  },
  "dependencies": {
    "@jbrowse/text-indexing-core": "workspace:^",
    "cli-progress": "^3.12.0",
    "command-exists": "^1.2.9",
    "cors": "^2.8.5",
    "decompress": "^4.2.1",
    "express": "^5.2.1",
    "ixixx": "^3.0.3",
    "json-parse-better-errors": "^1.0.2",
    "node-fetch-native": "^1.6.7",
    "tmp": "^0.2.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
