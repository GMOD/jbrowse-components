{
  "name": "@jbrowse/react-circular-genome-view2",
  "version": "4.1.5",
  "type": "module",
  "description": "JBrowse 2 circular genome view React component",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "MIT",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "products/jbrowse-react-circular-genome-view"
  },
  "author": "JBrowse Team",
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "esm",
    "dist",
    "docs"
  ],
  "scripts": {
    "prebuild": "pnpm clean",
    "build": "pnpm run /^build:/",
    "prepublishOnly": "node output-version.js > src/version.ts && git add -A src && git commit -m '[skip ci] Bump version.ts'",
    "prepack": "pnpm build",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:webpack": "webpack -c webpack.config.mjs",
    "clean": "rimraf esm dist *.tsbuildinfo",
    "storybook": "storybook dev --port 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@jbrowse/core": "workspace:^",
    "@jbrowse/embedded-core": "workspace:^",
    "@jbrowse/mobx-state-tree": "^5.5.0",
    "@jbrowse/plugin-authentication": "workspace:^",
    "@jbrowse/plugin-circular-view": "workspace:^",
    "@jbrowse/plugin-config": "workspace:^",
    "@jbrowse/plugin-data-management": "workspace:^",
    "@jbrowse/plugin-sequence": "workspace:^",
    "@jbrowse/plugin-variants": "workspace:^",
    "@jbrowse/plugin-wiggle": "workspace:^",
    "@jbrowse/product-core": "workspace:^",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "main": "esm/index.js",
    "types": "esm/index.d.ts",
    "exports": {
      ".": {
        "types": "./esm/index.d.ts",
        "import": "./esm/index.js"
      }
    }
  },
  "sideEffects": false
}
