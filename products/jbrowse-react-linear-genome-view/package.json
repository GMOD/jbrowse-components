{
  "name": "@jbrowse/react-linear-genome-view",
  "version": "2.2.1",
  "description": "JBrowse 2 linear genome view React component",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "MIT",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "products/jbrowse-react-linear-genome-view"
  },
  "author": "JBrowse Team",
  "distMain": "dist/index.js",
  "srcMain": "src/index.ts",
  "distModule": "esm/index.js",
  "srcModule": "src/index.ts",
  "module": "",
  "main": "src/index.ts",
  "files": [
    "dist",
    "src",
    "esm",
    "docs"
  ],
  "scripts": {
    "start:umd": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "npm-run-all build:*",
    "prepublishOnly": "node output-version.js > src/version.js && git add -A src && git commit -m '[skip ci] Bump version.js'",
    "build:esm": "tsc --build tsconfig.build.esm.json",
    "build:es5": "tsc --build tsconfig.build.es5.json",
    "build:webpack": "npm-run-all build:webpack:*",
    "build:webpack:dev": "cross-env NODE_ENV=development webpack",
    "build:webpack:prod": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist esm *.tsbuildinfo",
    "storybook": "start-storybook --static-dir public --port 6006",
    "storybook:build": "build-storybook --quiet --static-dir public",
    "prebuild": "yarn clean",
    "prepack": "yarn build && yarn useDist",
    "postpack": "yarn useSrc",
    "useDist": "node ../../scripts/useDist.js",
    "useSrc": "node ../../scripts/useSrc.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@jbrowse/core": "^2.2.1",
    "@jbrowse/plugin-alignments": "^2.2.1",
    "@jbrowse/plugin-authentication": "^2.2.1",
    "@jbrowse/plugin-bed": "^2.2.1",
    "@jbrowse/plugin-circular-view": "^2.2.1",
    "@jbrowse/plugin-config": "^2.2.1",
    "@jbrowse/plugin-data-management": "^2.2.1",
    "@jbrowse/plugin-gccontent": "^2.2.1",
    "@jbrowse/plugin-gff3": "^2.2.1",
    "@jbrowse/plugin-legacy-jbrowse": "^2.2.1",
    "@jbrowse/plugin-linear-genome-view": "^2.2.1",
    "@jbrowse/plugin-sequence": "^2.2.1",
    "@jbrowse/plugin-svg": "^2.2.1",
    "@jbrowse/plugin-trix": "^2.2.1",
    "@jbrowse/plugin-variants": "^2.2.1",
    "@jbrowse/plugin-wiggle": "^2.2.1",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "librpc-web-mod": "^1.1.5",
    "mobx": "^6.6.0",
    "mobx-react": "^7.5.0",
    "mobx-state-tree": "^5.0.0",
    "prop-types": "^15.0.0",
    "react-use-measure": "^2.1.1",
    "rxjs": "^6.0.0",
    "serialize-error": "^8.0.0",
    "tss-react": "^4.4.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
