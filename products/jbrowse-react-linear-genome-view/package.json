{
  "name": "@jbrowse/react-linear-genome-view",
  "version": "1.7.3",
  "description": "JBrowse 2 linear genome view React component",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "license": "MIT",
  "homepage": "https://jbrowse.org",
  "bugs": "https://github.com/GMOD/jbrowse-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/GMOD/jbrowse-components.git",
    "directory": "products/jbrowse-react-linear-genome-view"
  },
  "author": "JBrowse Team",
  "main": "dist/index.js",
  "files": [
    "dist",
    "src",
    "docs"
  ],
  "scripts": {
    "start:umd": "cross-env NODE_ENV=development webpack-dev-server",
    "prebuild": "npm run clean",
    "postbuild": "tsc -b tsconfig.build.json",
    "build": "npm-run-all build:*",
    "build:babel": "babel src --root-mode upward --out-dir dist --extensions .ts,.js,.tsx,.jsx",
    "build:webpack": "npm-run-all build:webpack:*",
    "build:webpack:dev": "cross-env NODE_ENV=development webpack",
    "build:webpack:prod": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist",
    "prepack": "npm run build",
    "storybook": "start-storybook --static-dir public --port 6006",
    "storybook:build": "build-storybook --quiet --static-dir public"
  },
  "dependencies": {
    "@jbrowse/core": "^1.7.3",
    "@jbrowse/plugin-alignments": "^1.7.3",
    "@jbrowse/plugin-bed": "^1.7.3",
    "@jbrowse/plugin-circular-view": "^1.7.3",
    "@jbrowse/plugin-config": "^1.7.3",
    "@jbrowse/plugin-data-management": "^1.7.3",
    "@jbrowse/plugin-gff3": "^1.7.3",
    "@jbrowse/plugin-linear-genome-view": "^1.7.3",
    "@jbrowse/plugin-sequence": "^1.7.3",
    "@jbrowse/plugin-svg": "^1.7.3",
    "@jbrowse/plugin-trix": "^1.7.3",
    "@jbrowse/plugin-variants": "^1.7.3",
    "@jbrowse/plugin-wiggle": "^1.7.3",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.0.0",
    "@material-ui/lab": "^4.0.0-alpha.45",
    "mobx": "^5.10.1",
    "mobx-react": "^6.0.3",
    "mobx-state-tree": "3.14.1",
    "prop-types": "^15.0.0",
    "react-use-measure": "^2.1.1",
    "rxjs": "^6.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
