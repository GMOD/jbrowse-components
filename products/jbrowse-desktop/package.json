    "@jbrowse/plugin-circular-view": "^2.6.3",
    "@jbrowse/plugin-comparative-adapters": "^2.6.3",
    "@jbrowse/plugin-config": "^2.6.3",
    "@jbrowse/plugin-data-management": "^2.6.3",
    "@jbrowse/plugin-dotplot-view": "^2.6.3",
    "@jbrowse/plugin-gccontent": "^2.6.3",
