    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^17.0.0",
    "react-error-boundary": "^4.0.3",
    "react-refresh": "^0.11.0",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "rxjs": "^7.0.0",
    "sanitize-filename": "^1.6.3",
    "sass-loader": "^12.3.0",
    "semver": "^7.3.5",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.2",
    "terser-webpack-plugin": "^5.2.5",
    "timeago.js": "^4.0.2",
    "tss-react": "^4.4.1",
    "use-query-params": "^2.0.0",
    "webpack": "^5.64.4",
    "webpack-dev-server": "^4.6.0",
    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "devDependencies": {
    "electron": "25.5.0"
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "build": {
    "electronVersion": "25.5.0",
    "extraMetadata": {
      "main": "build/electron.js"
    },
    "appId": "org.jbrowse2.app",
    "productName": "JBrowse 2",
    "copyright": "Copyright Â© 2019",
    "snap": {
      "confinement": "strict",
      "plugs": [
        "default",
        "removable-media"
      ],
      "slots": [
        "default"
      ]
    },
    "win": {
      "publish": [
        "github"
      ],
      "artifactName": "jbrowse-desktop-v${version}-win.${ext}"
    },
    "linux": {
      "publish": [
        "github"
      ],
      "executableName": "jbrowse-desktop",
      "artifactName": "jbrowse-desktop-v${version}-linux.${ext}"
    },
    "mac": {
      "publish": [
        "github"
      ],
      "category": "public.app-category.utilities",
      "artifactName": "jbrowse-desktop-v${version}-mac.${ext}"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets/"
    },
    "afterSign": "scripts/notarize.js"
  },
  "private": true,
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
