{
  "compilerOptions": {
    "target": "es2018",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noUncheckedIndexedAccess": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "module": "esnext",
    "removeComments": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "isolatedModules": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true,
    "baseUrl": ".",
    "paths": {
      "@jbrowse/core/*": ["packages/core/*"],
      "@jbrowse/product-core/*": ["packages/product-core/*"],
      "@jbrowse/app-core/*": ["packages/app-core/*"],
      "@jbrowse/web-core/*": ["packages/web-core/*"],
      "@jbrowse/embedded-core/*": ["packages/embedded-core/*"],
      "@jbrowse/sv-core/*": ["packages/sv-core/*"],
      "@jbrowse/text-indexing/*": ["packages/text-indexing/*"],
      "@jbrowse/web/*": ["products/jbrowse-web/*"],
      "@jbrowse/plugin-alignments": ["plugins/alignments/src/index.ts"],
      "@jbrowse/plugin-alignments/*": ["plugins/alignments/*"],
      "@jbrowse/plugin-arc": ["plugins/arc/src/index.ts"],
      "@jbrowse/plugin-arc/*": ["plugins/arc/*"],
      "@jbrowse/plugin-authentication": ["plugins/authentication/src/index.ts"],
      "@jbrowse/plugin-authentication/*": ["plugins/authentication/*"],
      "@jbrowse/plugin-bed": ["plugins/bed/src/index.ts"],
      "@jbrowse/plugin-bed/*": ["plugins/bed/*"],
      "@jbrowse/plugin-breakpoint-split-view": [
        "plugins/breakpoint-split-view/src/index.ts"
      ],
      "@jbrowse/plugin-breakpoint-split-view/*": [
        "plugins/breakpoint-split-view/*"
      ],
      "@jbrowse/plugin-canvas": ["plugins/canvas/src/index.ts"],
      "@jbrowse/plugin-canvas/*": ["plugins/canvas/*"],
      "@jbrowse/plugin-circular-view": ["plugins/circular-view/src/index.ts"],
      "@jbrowse/plugin-circular-view/*": ["plugins/circular-view/*"],
      "@jbrowse/plugin-comparative-adapters": [
        "plugins/comparative-adapters/src/index.ts"
      ],
      "@jbrowse/plugin-comparative-adapters/*": [
        "plugins/comparative-adapters/*"
      ],
      "@jbrowse/plugin-config": ["plugins/config/src/index.ts"],
      "@jbrowse/plugin-config/*": ["plugins/config/*"],
      "@jbrowse/plugin-data-management": [
        "plugins/data-management/src/index.ts"
      ],
      "@jbrowse/plugin-data-management/*": ["plugins/data-management/*"],
      "@jbrowse/plugin-dotplot-view": ["plugins/dotplot-view/src/index.ts"],
      "@jbrowse/plugin-dotplot-view/*": ["plugins/dotplot-view/*"],
      "@jbrowse/plugin-gccontent": ["plugins/gccontent/src/index.ts"],
      "@jbrowse/plugin-gccontent/*": ["plugins/gccontent/*"],
      "@jbrowse/plugin-gff3": ["plugins/gff3/src/index.ts"],
      "@jbrowse/plugin-gff3/*": ["plugins/gff3/*"],
      "@jbrowse/plugin-grid-bookmark": ["plugins/grid-bookmark/src/index.ts"],
      "@jbrowse/plugin-grid-bookmark/*": ["plugins/grid-bookmark/*"],
      "@jbrowse/plugin-gtf": ["plugins/gtf/src/index.ts"],
      "@jbrowse/plugin-gtf/*": ["plugins/gtf/*"],
      "@jbrowse/plugin-hic": ["plugins/hic/src/index.ts"],
      "@jbrowse/plugin-hic/*": ["plugins/hic/*"],
      "@jbrowse/plugin-jobs-management": [
        "plugins/jobs-management/src/index.ts"
      ],
      "@jbrowse/plugin-jobs-management/*": ["plugins/jobs-management/*"],
      "@jbrowse/plugin-legacy-jbrowse": ["plugins/legacy-jbrowse/src/index.ts"],
      "@jbrowse/plugin-legacy-jbrowse/*": ["plugins/legacy-jbrowse/*"],
      "@jbrowse/plugin-linear-comparative-view": [
        "plugins/linear-comparative-view/src/index.ts"
      ],
      "@jbrowse/plugin-linear-comparative-view/*": [
        "plugins/linear-comparative-view/*"
      ],
      "@jbrowse/plugin-linear-genome-view": [
        "plugins/linear-genome-view/src/index.ts"
      ],
      "@jbrowse/plugin-linear-genome-view/*": ["plugins/linear-genome-view/*"],
      "@jbrowse/plugin-lollipop": ["plugins/lollipop/src/index.ts"],
      "@jbrowse/plugin-lollipop/*": ["plugins/lollipop/*"],
      "@jbrowse/plugin-menus": ["plugins/menus/src/index.ts"],
      "@jbrowse/plugin-menus/*": ["plugins/menus/*"],
      "@jbrowse/plugin-rdf": ["plugins/rdf/src/index.ts"],
      "@jbrowse/plugin-rdf/*": ["plugins/rdf/*"],
      "@jbrowse/plugin-sequence": ["plugins/sequence/src/index.ts"],
      "@jbrowse/plugin-sequence/*": ["plugins/sequence/*"],
      "@jbrowse/plugin-spreadsheet-view": [
        "plugins/spreadsheet-view/src/index.ts"
      ],
      "@jbrowse/plugin-spreadsheet-view/*": ["plugins/spreadsheet-view/*"],
      "@jbrowse/plugin-sv-inspector": ["plugins/sv-inspector/src/index.ts"],
      "@jbrowse/plugin-sv-inspector/*": ["plugins/sv-inspector/*"],
      "@jbrowse/plugin-text-indexing": ["plugins/text-indexing/src/index.ts"],
      "@jbrowse/plugin-text-indexing/*": ["plugins/text-indexing/*"],
      "@jbrowse/plugin-trix": ["plugins/trix/src/index.ts"],
      "@jbrowse/plugin-trix/*": ["plugins/trix/*"],
      "@jbrowse/plugin-variants": ["plugins/variants/src/index.ts"],
      "@jbrowse/plugin-variants/*": ["plugins/variants/*"],
      "@jbrowse/plugin-wiggle": ["plugins/wiggle/src/index.ts"],
      "@jbrowse/plugin-wiggle/*": ["plugins/wiggle/*"]
    }
  },
  "exclude": [
    "**/build/",
    "**/docs/",
    "**/dist/",
    "**/esm/",
    "**/lib/",
    "**/umd/",
    "**/esm/",
    "**/tmp/",
    "**/config/jest/",
    "**/embedded_demos/",
    "**/component_tests/",
    "**/plugin-development-tools/",
    "**/webpack.config.js",
    "**/website",
    "**/auth_test_utils"
  ]
}
